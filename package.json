{
  "name": "s3-invalidate-cloudfront",
  "description": "Invalidate CloudFront caches for a bucket.",
  "version": "0.2.0",
  "license": "MIT",
  "author": "Joel Lefkowitz",
  "repository": "https://github.com/joellefkowitz/s3-invalidate-cloudfront",
  "homepage": "https://joellefkowitz.github.io/s3-invalidate-cloudfront",
  "main": "dist/index.js",
  "bin": "dist/main.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "aws",
    "s3",
    "cloudfront"
  ],
  "packageManager": "yarn@4.1.1",
  "metadata": {
    "publisher": "npm",
    "languages": [
      "TypeScript"
    ],
    "frameworks": [],
    "paradigms": [
      "Functional"
    ],
    "lifecycle": "Alpha"
  },
  "scripts": {
    "postinstall": "husky",
    "lint": "cspell . --dot --gitignore && eslint . --fix && tsc --noEmit && trufflehog3 -c .trufflehog3.yaml",
    "format": "prettier . --write",
    "build": "tsc",
    "test": "vitest --run --coverage",
    "docs": "typedoc --options docs/typedoc/typedoc.json && tsx ./docs/typedoc/assets.ts"
  },
  "resolutions": {
    "cliui": "^8.0.1",
    "strip-ansi": "^6.0.1"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.927.0",
    "ramda": "^0.32.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/json-stringify-safe": "^5",
    "@types/node": "^18.11.0",
    "@types/ramda": "^0",
    "@types/yargs": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitest/coverage-v8": "4.0.8",
    "cspell": "^8.0.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "glob": "^11.0.0",
    "husky": "^9.0.0",
    "pinst": "^3.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-jsdoc": "^1.3.0",
    "tslib": "^2.3.0",
    "tsx": "^4.20.6",
    "typedoc": "^0.25.7",
    "typedoc-material-theme": "^1.0.2",
    "typescript": "~5.3.0",
    "vitest": "^4.0.8"
  }
}
